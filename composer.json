{
    "name": "dynamic/recipe-silverstripe-foxy",
    "description": "Full install of silverstripe-foxy ecommerce platform",
    "type": "silverstripe-recipe",
    "keywords": [
        "silverstripe",
        "foxy",
        "ecommerce"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "require": {
        "silverstripe/recipe-cms": "^4.0",
        "dynamic/silverstripe-foxy": "^1.0",
        "dynamic/silverstripe-foxy-api": "^1.0",
        "dynamic/silverstripe-foxy-discounts": "^1.0",
        "dynamic/silverstripe-foxy-feed-parser": "^1.0",
        "dynamic/silverstripe-foxy-integrations": "^1.0",
        "dynamic/silverstripe-foxy-inventory": "^1.0",
        "dynamic/silverstripe-foxy-orders": "^1.0",
        "dynamic/silverstripe-foxy-products": "^1.0",
        "dynamic/silverstripe-foxy-single-sign-on": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "theseer/phpdox": "^0.11.0"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "project-files": ["app/*"],
        "expose": [
            "client/dist"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
